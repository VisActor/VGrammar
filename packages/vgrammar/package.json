{
  "name": "@visactor/vgrammar",
  "version": "0.5.1",
  "description": "VGrammar is a visual grammar library",
  "keywords": [
    "grammar",
    "visualization",
    "VGrammar",
    "storytelling",
    "VisActor"
  ],
  "author": {
    "name": "VisActor",
    "url": "https://VisActor.io/"
  },
  "license": "MIT",
  "sideEffects": false,
  "main": "cjs/index.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "files": [
    "cjs",
    "es",
    "dist"
  ],
  "scripts": {
    "compile": "tsc --noEmit",
    "eslint": "eslint --debug --fix src/",
    "build": "bundle --clean",
    "build:ci": "bundle --clean -f umd --minify=false -n ChartSpace",
    "dev": "bundle --clean -f es -w",
    "start": "vite ./vite",
    "test": "jest",
    "test-cov": "jest -w 16 --coverage",
    "test-check": "DEBUG=jest jest --forceExit --detectOpenHandles --silent false --verbose false --runInBand",
    "ci": "ts-node --transpileOnly --skipProject ./scripts/trigger-test.ts"
  },
  "dependencies": {
    "@visactor/vgrammar-coordinate": "workspace:0.5.1",
    "@visactor/vrender": "~0.14.0",
    "@visactor/vgrammar-util": "workspace:0.5.1",
    "@visactor/vscale": "~0.13.3",
    "@visactor/vrender-components": "~0.14.0",
    "@visactor/vutils": "~0.13.3"
  },
  "devDependencies": {
    "@internal/bundler": "workspace:*",
    "@internal/eslint-config": "workspace:*",
    "@internal/ts-config": "workspace:*",
    "@internal/jest-config": "workspace:*",
    "@rushstack/eslint-patch": "~1.1.4",
    "eslint": "~8.18.0",
    "jest": "~29.5.0",
    "vite": "3.2.6",
    "jest-environment-jsdom": "~29.5.0",
    "@jest/globals": "~29.5.0",
    "ts-jest": "~29.1.0",
    "@types/jest": "~29.5.0",
    "typescript": "4.9.5",
    "@types/node": "*",
    "node-fetch": "2.6.7",
    "form-data": "~4.0.0",
    "ts-node": "10.9.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
